{
  "name": "ask",
  "version": "0.1.0",
  "private": true,
  "author": "Joimee",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "fix:eslint": "eslint --fix ./ --ext .js --ext .jsx",
    "fix:stylelint": "stylelint --fix ./**/*.{css,scss,sass}",
    "fix": "npm run fix:eslint && npm run fix:stylelint",
    "component": "node ./internals/component.js",
    "prisma:migrate": "npx prisma migrate save --experimental",
    "prisma:migrate:up": "npx prisma migrate up --experimental",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:update": "yarn prisma:migrate && yarn prisma:migrate:up && yarn prisma:generate",
    "prisma:reset": "sh ./internals/db-clean.sh"
  },
  "dependencies": {
    "@prisma/client": "2.10.2",
    "@zeit/next-sass": "^1.0.1",
    "lodash.throttle": "^4.1.1",
    "next": "9.3.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-svg": "^11.1.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@prisma/cli": "2.10.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "node-sass": "^4.13.1",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.16.0"
  }
}
